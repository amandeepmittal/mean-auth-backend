{
  "name": "mean-auth-backend-boilerplate",
  "version": "1.0.0",
  "description": "User Auth RESTful API for MEAN Stack applications",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "log": "changelog",
    "dev": "nodemon ./bin/www",
    "lint": "eslint */**.js; exit 0 ",
    "prod": "NODE_ENV=production ./bin/www",
    "start": "node ./bin/www",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "cors": "2.8.4",
    "debug": "~3.0.1",
    "eslint": "4.5.0",
    "express": "~4.15.2",
    "jade": "~1.11.0",
    "jwt-simple": "0.5.1",
    "moment": "2.18.1",
    "mongoose": "4.11.9",
    "morgan": "~1.8.1",
    "snyk": "^1.29.0"
  },
  "devDependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog-emoji": "0.1.0",
    "eslint": "4.5.0",
    "eslint-plugin-one-variable-per-var": "0.0.3",
    "generate-changelog": "1.3.1",
    "nodemon": "1.11.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog-emoji"
    }
  },
  "snyk": true
}
